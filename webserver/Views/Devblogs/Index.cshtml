@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid m-2">
    <div class="row">
        <div class="col-10">
            <h1>Developer blog</h1>
        </div>
        <div class="col-2">
            <button class="btn btn-dark">
                <a class="nav-link text-light font-weight-bold" asp-area="" asp-controller="DevBlogs" asp-action="Create">
                    New post
                </a>
            </button>
        </div>

        <div id="content" class="container"/>
    </div>
</div>

@section Scripts
{
<script>
    $(function () {

        $("#content").load("DevBlogs/GetDevBlog");
        return false;
    });
</script>
}
